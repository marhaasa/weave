{
  "name": "weave",
  "version": "0.1.16",
  "description": "An interactive TUI for Microsoft Fabric CLI",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "weave": "./bin/weave"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && node dist/index.js",
    "dev": "tsx src/index.tsx",
    "dev:watch": "tsx watch src/index.tsx",
    "dev:old": "tsx weave.ts",
    "watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "ink": "^4.4.1",
    "react": "^18.3.1"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "keywords": [
    "fabric",
    "cli",
    "tui",
    "microsoft",
    "terminal"
  ],
  "author": "Marius Høgli Aasarød",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/marhaasa/weave.git"
  },
  "bugs": {
    "url": "https://github.com/marhaasa/weave/issues"
  },
  "homepage": "https://github.com/marhaasa/weave#readme",
  "devDependencies": {
    "@types/node": "^22.15.21",
    "@types/react": "^19.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.8.3"
  },
  "files": [
    "dist",
    "bin",
    "README.md",
    "LICENSE"
  ]
}
